{
  "manifest_version": 3,
  "name": "Shop-E-Sense",
  "version": "1.0",
  "description": "Finds all the dark patterns on a website.",
  "permissions": [
    "activeTab",
    "webRequest",
    "tabs",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}